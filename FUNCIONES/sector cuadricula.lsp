(DEFUN seccuad (listapod lx ly nx ny x1 y1 px py)
  (setq sx (fix (1+ (/ (- px x1) (/ lx nx)))))
  (setq sy (fix (1+ (/ (- py y1) (/ ly ny)))))
  (setq numerocelda (1- (+ sx  (* nx (1- sy)))))
  (setq celdasel (substr listapod (1+ (* numerocelda (/ (strlen listapod) (* nx ny)))) (/ (strlen listapod) (* nx ny))))
)

;listapod	cadena		lista de apodos para las celdas escritos de izquierda a derecha de arriba hacia abajo y con un ancho "n" pero constante en toda la cadena
;lx		nummero		longitud del rectangulo en la direccion x en pixeles
;ly		nummero		longitud del rectangulo en la direccion y en pixeles
;nx		nummero		numero de celdas existentes en x
;ny		nummero		numero de celdas existentes en y
;x1		numero		coordenada inicial del rectangulo en x
;y1		numero		coordenada inicial del rectangulo en y
;px		numero		pulsacion en coordenada x
;py		numero		pulsacion en coordenada y

